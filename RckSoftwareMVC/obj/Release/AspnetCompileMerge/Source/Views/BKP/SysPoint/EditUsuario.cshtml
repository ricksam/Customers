@model RckSoftwareMVC.Models.SysPoint.Colaboradores
@{
    ViewBag.Title = "Colaboradores";
}

<div class="container">
    <div class="page-header">
        <h1>@ViewBag.Title</h1>
    </div>

    <div class="form-group">
        <label class="col-sm-2">Nome</label>
        <div class="col-sm-10"><p class="form-control-static">@Model.Nome</p></div>
    </div>
    <div class="form-group">
        <label class="col-sm-2">Senha</label>
        <div class="col-sm-10"><p class="form-control-static">@Model.Senha</p></div>
    </div>
</div>

