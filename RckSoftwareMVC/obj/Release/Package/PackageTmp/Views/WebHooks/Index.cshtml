@{
    ViewBag.Title = "Index";
    string date = DateTime.Now.ToString("yyMMddHHmmss");
}

<div class="container">
    <form action="/mem/save" method="post">
        <div class="card form-group">
            <div class="card-header">
            </div>
            <div class="card-body">
                @Html.Raw(ViewBag.Content)
                <br />
                <a class="btn btn-secondary" href="/webhooks?@(date)">Reload</a>
            </div>
        </div>
    </form>
</div>
